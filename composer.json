{
    "name": "technobase/watchdog",
    "description": "Laravel package for sending error notifications to Telegram channels with detailed context and stack traces",
    "type": "library",
    "keywords": [
        "laravel",
        "telegram",
        "error-notification",
        "error-tracking",
        "monitoring",
        "watchdog",
        "technobase"
    ],
    "homepage": "https://github.com/technobase/watchdog",
    "license": "MIT",
    "authors": [
        {
            "name": "Technobase",
            "email": "dev@technobase.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/notifications": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "laravel-notification-channels/telegram": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Technobase\\Watchdog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Technobase\\Watchdog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Technobase\\Watchdog\\WatchdogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
